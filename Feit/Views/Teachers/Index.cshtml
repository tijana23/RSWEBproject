@model Feit.ViewModels.TeacherViewModel@{ ViewData["Title"] = "Index";}<h1>Teachers</h1><form asp-controller="Teachers" asp-action="Create" method="get">    <button type="submit" class="btn btn-primary">Add new teacher</button></form><h1> </h1><form asp-controller="Teachers" asp-action="Index" method="get">    <p>        First Name: <input type="text" asp-for="SearchFirstName" /> <input type="submit" value="Apply" class="btn-primary"  />        Last Name: <input type="text" asp-for="SearchLastName" /> <input type="submit" value="Apply" class="btn-primary"  />        Degree: <input type="text" asp-for="SearchDegree" /> <input type="submit" value="Apply" class="btn-primary"  /><br />        Academic Rank: <input type="text" asp-for="SearchAcademicRank" /> <input type="submit" value="Apply" class="btn-primary"  />    </p></form><table class="table">    <thead>        <tr>            <th>                First Name            </th>            <th>                Last Name            </th>            <th>                Degree            </th>            <th>                Academic Rank            </th>            <th>                Office Number            </th>            <th>                Hire Date            </th>            <th></th>        </tr>    </thead>    <tbody>        @foreach (var item in Model.Teachers)        {<tr>    <td>        @Html.DisplayFor(modelItem => item.FirstName)    </td>    <td>        @Html.DisplayFor(modelItem => item.LastName)    </td>    <td>        @Html.DisplayFor(modelItem => item.Degree)    </td>    <td>        @Html.DisplayFor(modelItem => item.AcademicRank)    </td>    <td>        @Html.DisplayFor(modelItem => item.OfficeNumber)    </td>    <td>        @Html.DisplayFor(modelItem => item.HireDate)    </td>    <td>        <form asp-action="Courses" asp-route-id="@item.Id" method="get">            <button type="submit" class="btn btn-primary">courses</button>        </form>        <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |        <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |        <a asp-action="Details" asp-route-id="@item.Id">Details</a> |        <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>    </td></tr>}    </tbody></table>